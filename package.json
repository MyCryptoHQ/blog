{
  "name": "@mycrypto/blog",
  "version": "1.0.0",
  "main": "index.js",
  "author": "MyCrypto",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "deploy": "gatsby-plugin-s3 deploy",
    "lint": "yarn run lint:types && yarn run lint:source && yarn run lint:format",
    "lint:types": "tsc --noEmit",
    "lint:source": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx,.mdx",
    "lint:format": "prettier --check --ignore-path .gitignore \"**/*.{ts,tsx,js,json,yml,mdx}\"",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js,json,yml,mdx}\"",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^3.12.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/lunr": "^2.3.4",
    "@types/react-helmet": "^6.1.4",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-preset-gatsby": "^2.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^4.1.0",
    "gatsby-plugin-canonical-urls": "^4.1.0",
    "gatsby-plugin-feed": "^4.1.0",
    "gatsby-plugin-image": "^2.1.0",
    "gatsby-plugin-local-search": "^2.0.1",
    "gatsby-plugin-manifest": "^4.1.0",
    "gatsby-plugin-matomo": "^0.10.0",
    "gatsby-plugin-nprogress": "^4.1.0",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^4.1.0",
    "gatsby-plugin-styled-components": "^5.1.0",
    "gatsby-plugin-ts-config": "^2.1.3",
    "gatsby-source-filesystem": "^4.1.0",
    "gatsby-source-ghost": "^4.2.4",
    "gatsby-transformer-sharp": "^4.1.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "rehype-parse": "^7.0.1",
    "rehype-stringify": "^8.0.0",
    "simple-git-hooks": "^2.7.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "unified": "^9.2.2",
    "unist-util-visit": "^2.0.3"
  },
  "dependencies": {
    "@fontsource/lato": "^4.5.0",
    "@lingui/core": "^3.12.1",
    "@lingui/macro": "^3.12.1",
    "@lingui/react": "^3.12.1",
    "@mycrypto/ui": "^1.9.2",
    "babel-runtime": "^6.26.0",
    "lodash.merge": "^4.6.2",
    "lunr": "^2.3.9",
    "make-plural": "^7.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,md,mdx}": [
      "prettier --write"
    ]
  }
}
